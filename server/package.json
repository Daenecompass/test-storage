{
  "name": "test-storage-backend",
  "version": "0.42.0",
  "description": "Test Case Management System backend",
  "author": "Dmitry Zakharov",
  "license": "GPLv3",
  "private": true,
  "keywords": [
    "test management",
    "test case management"
  ],
  "bugs": {
    "url": "https://github.com/pumano/test-storage/issues"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "prebuild:old": "ts-node ./src/modules/jwt-init.ts",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:old": "set NODE_ENV=production&&node ./dist/main.js",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "clean": "rimraf node_modules dist package-lock.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.1.1",
    "@nestjs/config": "0.5.0",
    "@nestjs/core": "^7.1.1",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/mongoose": "^7.0.1",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.1.1",
    "@nestjs/platform-socket.io": "^7.1.1",
    "@nestjs/swagger": "^4.5.9",
    "@nestjs/websockets": "^7.1.1",
    "bcrypt": "4.0.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "database-updates": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.16",
    "multer": "^1.4.2",
    "passport": "0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "socket.io": "^2.3.0",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.2.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.1.1",
    "@types/bcrypt": "^3.0.0",
    "@types/config": "0.0.36",
    "@types/faker": "^4.1.12",
    "@types/jest": "^25.2.3",
    "@types/node": "~14.0.6",
    "@types/supertest": "^2.0.9",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "typescript": "^3.6.3"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
